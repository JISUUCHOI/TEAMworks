<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC 
"-//mybatis.org//DTD Config 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>

	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.kh.teamworks.board.model.vo.Board" alias="Board"/>
		<typeAlias type="com.kh.teamworks.employee.model.vo.Employee" alias="Employee"/>
		<typeAlias type="com.kh.teamworks.approval.model.vo.Document" alias="Document"/>
		<typeAlias type="com.kh.teamworks.reservation.model.vo.Reservation" alias="Reservation"/>
		<typeAlias type="com.kh.teamworks.schedule.model.vo.Schedule" alias="Schedule"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="mappers/board-mapper.xml"/>
		<mapper resource="mappers/employee-mapper.xml"/>
		<mapper resource="mappers/reservation-mapper.xml"/>
		<mapper resource="mappers/schedule-mapper.xml"/>
		<mapper resource="mappers/document-mapper.xml"/>
	</mappers>

</configuration>
